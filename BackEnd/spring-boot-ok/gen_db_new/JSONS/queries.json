SELECT count(*) FROM CANDIDAT    group by  JSON_EXTRACT(candidat, '$.CNP');
SELECT count(*) FROM EXAMEN      group by  JSON_EXTRACT(examen, '$.ID');
SELECT count(*) FROM MEDIE       group by  JSON_EXTRACT(medie, '$.CANDIDATCNP');
SELECT count(*) FROM NOTE        group by  JSON_EXTRACT(note, '$.CANDIDATCNP');
SELECT count(*) FROM PROFESORI   group by  JSON_EXTRACT(profesori, '$.PCNP');
SELECT count(*) FROM SALI        group by  JSON_EXTRACT(sali, '$.ID');
SELECT count(*) FROM SALI_EXAMEN group by  JSON_EXTRACT(sali_examen, '$.SALIID');
SELECT count(*) FROM NOTIFICARI  group by  JSON_EXTRACT(notificari, '$.TEXT');
SELECT count(*) FROM USER        group by  JSON_EXTRACT(user, '$.ROL');
SELECT count(*) FROM FORM        where JSON_EXTRACT(form, '$.STATUS') like 'acceptat%' group by  JSON_EXTRACT(form, '$.STATUS');
SELECT count(*) FROM FORM        where JSON_EXTRACT(form, '$.STATUS') like 'date%' group by  JSON_EXTRACT(form, '$.STATUS');
SELECT count(*) FROM FORM        where JSON_EXTRACT(form, '$.STATUS') like 'respins%' group by  JSON_EXTRACT(form, '$.STATUS');
